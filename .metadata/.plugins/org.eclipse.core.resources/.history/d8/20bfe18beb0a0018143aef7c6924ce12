package week1;

public class day2_test1 {
	public static void main(String[] args) {
		String name = "구명회";
		int numberLike = 0;
		double leftEye = 0.8, rightEye = 0.8;
		String foodLike = "갈비";
		String sportsLike = "볼링";
		String alphabetLike = "S";
		Boolean songLike = true;
				
		System.out.println("이름 : " + name);
		System.out.println("좋아하는 숫자 : " + numberLike);
		System.out.println("시력 - 좌 : " +  leftEye + ", 우 : " + rightEye);
		System.out.println("좋아하는 음식 : " + foodLike);
		System.out.println("좋아하는 음식 : " + sportsLike);
		System.out.println("좋아하는 음식 : " + alphabetLike);
		System.out.println("좋아하는 음식 : " + songLike);
	}
}
