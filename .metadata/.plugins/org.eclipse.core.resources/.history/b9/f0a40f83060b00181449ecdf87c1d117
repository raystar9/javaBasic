package week1.day2;

public class Test3 {

	public static void main(String[] args) {
		
		int a = 20, b = 6;
		int num = 0;
		
		num = a + b;
		System.out.printf("%d + %d = %d\n", a, b, num);
		num = a - b;
		System.out.printf("%d - %d = %d\n", a, b, num);
		num = a * b;
		System.out.printf("%d * %d = %d\n", a, b, num);
		num = a / b;
		System.out.printf("%d / %d = %d\n", a, b, num);
		num = a % b;
		System.out.printf("%d %c %d = %d", a, '%', b, num);
	}
}