package week1;

public class day2_variable {
	public static void main(String[] args) {
		byte b = 10;
		short s = 100;
		int i;
		
		long l = 10000L;
		float ft;
		
		ft = 3.14f;
		double d = 42.195d;
		
		b = 20;
		System.out.println("b = " + b);
		b += 300;
		System.out.println("b = " + b);
		
		System.out.println(i);
	}
}
