package week1.day2;

public class Test3 {

	public static void main(String[] args) {
		
		int a = 10, b = 4;
		int num = 0;
		num = a + b;
		System.out.printf("%d + %d = %d\n", a, b, num);
		num = a - b;
		System.out.printf("%d - %d = %d\n", a, b, num);
		num = a * b;
		System.out.printf("%d * %d = %d\n", a, b, num);
		num = a / b;
		System.out.printf("%d / %d = %d\n", a, b, num);
	}
}
